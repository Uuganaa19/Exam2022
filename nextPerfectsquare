import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		int i = 36;
		
		int j = 1;
		int square;
		
		do {
			square = j * j;
			j++;
			System.out.println("Square : " + square);
		}while (i > square);
		
		if (i < square){
			System.out.println("Next perfect Square is : " + square);
		}a
		if (i == square){
			square = j * j;
			System.out.println("Next perfect Square is : " + square);
		}
	}
}
